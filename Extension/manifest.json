{
    "name": "OTN Modern Client for Chrome",
    "version": "0.0.1.88",
    "description": "The OTN Modern Client straight in Your Chrome Browser! Manage printing jobs while watching cat videos!",
    "permissions": ["storage", "downloads"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icon16.png",
            "32": "images/icon32.png",
            "48": "images/icon48.png",
            "128": "images/icon128.png"
        }
    },
    "icons": {
        "16": "images/icon16.png",
        "32": "images/icon32.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "manifest_version": 2
}